<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <SwitchPreference
        android:defaultValue="@bool/play_default"
        android:key="@string/key_play"
        android:summary="@string/summary_play"
        android:title="@string/title_play"
        >
    </SwitchPreference>
    <SwitchPreference
        android:defaultValue="@bool/clipboard_default"
        android:key="@string/key_clipboard"
        android:summary="@string/summary_clipboard"
        android:title="@string/title_clipboard">
    </SwitchPreference>
    <SwitchPreference
        android:defaultValue="@bool/search_default"
        android:key="@string/key_search"
        android:summary="@string/summary_search"
        android:title="@string/title_search">
    </SwitchPreference>
    <SwitchPreference
        android:defaultValue="@bool/appops_default"
        android:key="@string/key_appops"
        android:summary="@string/summary_appops"
        android:title="@string/title_appops">
    </SwitchPreference>
    <SwitchPreference
        android:defaultValue="@bool/appsettings_default"
        android:key="@string/key_appsettings"
        android:summary="@string/summary_appsettings"
        android:title="@string/title_appsettings">
    </SwitchPreference>
    <PreferenceCategory android:title="Enhance">
        <SwitchPreference
            android:defaultValue="@bool/show_dismiss_default"
            android:key="@string/key_show_dismiss"
            android:summary="@string/summary_show_dismiss"
            android:title="@string/title_show_dismiss">
        </SwitchPreference>
        <SwitchPreference
            android:defaultValue="@bool/forcestop_default"
            android:key="@string/key_forcestop"
            android:summary="@string/summary_forcestop"
            android:title="@string/title_forcestop">
        </SwitchPreference>
    </PreferenceCategory>

    <PreferenceCategory android:title="@string/title_about">
        <Preference
            android:selectable="false"
            android:title="@string/app_name"/>
        <Preference
            android:icon="@drawable/xda_icon"
            android:title="@string/title_xda"/>
    </PreferenceCategory>
</PreferenceScreen>